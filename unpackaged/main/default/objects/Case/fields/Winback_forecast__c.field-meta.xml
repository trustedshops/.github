<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Winback_forecast__c</fullName>
    <description>extract number of winback status, divide through 100 and multiply with yearly price</description>
    <externalId>false</externalId>
    <formula>((VALUE(LEFT(TEXT(Winback_Status__c),FIND(&quot;_&quot;,TEXT(Winback_Status__c))-1)))/100)* Yearly_price_of_new_offer__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Winback forecast</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
